<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd"> 

<mapper namespace="com.vinflux.mobile.admin.info">

	<parameterMap id="parameterMap" type="java.util.HashMap" />
 	<resultMap id="resultMap" type="java.util.HashMap" />

	<select id="SelectCountryInfo" parameterMap="parameterMap" resultMap="resultMap">
		<![CDATA[
			SELECT *
			  FROM TADMIN_MST_CT
			 WHERE COUNTRYCODE = #{COUNTRYCODE}
		]]>		
	</select>

	<select id="SelectCodeInfo" parameterMap="parameterMap" resultMap="resultMap">
		<![CDATA[
			SELECT ADCD_DTKEY, ADCD_DTNAME
			  FROM TADMIN_MST_ADCD_DT
			 WHERE ADCD_HDKEY   = #{ADCD_HDKEY}
			   AND LAKEY        = #{LAKEY}
		]]>		
	</select>


</mapper>